{
  "name": "@jpx/shared",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./styles": "./src/styles/index.css",
    "./test": "./src/test/index.ts",
    ".": "./dist/index.js"
  },
  "style": "src/styles/index.css",
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
