    "clean": "rm -rf dist node_modules/.tsbuildinfo"
  },
  "dependencies": {
    "tslog": "^4.10.2",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.2",
    "@types/validator": "^13.15.10",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "eslint": "^9.0.0",
    "typescript": "^5.9.3"
  }
}
